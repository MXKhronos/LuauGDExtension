{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "scons target=editor",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            },
            "problemMatcher": "$gcc"
        },
        {
            "label": "run",
            "type": "shell",
            "command": "godot --path Y:/Repo/luau_gamedemo -e -d",
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "procdump",
            "type": "shell",
            "command": "procdump -e -x ./dumps Z:/Godot/Godot_v4.4-stable_mono_win64/Godot_v4.4-stable_mono_win64.exe --path Y:/Repo/luau_gamedemo -e -d",
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
}
